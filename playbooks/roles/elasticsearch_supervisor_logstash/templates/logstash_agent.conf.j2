input {
  file{
    type=>"nginx-access"
    path=>"log/path"
    codec => json {
      charset => "UTF-8"
    }
  }
}
filter {
  if [type] == 'nginx-access' {
    json {
      source => "@fields"
    }
  }
}
output {
    redis {
      host => "redis-hosts"
      port => "6379"
      db => "0"
      data_type => "list"
      key => "nginx:redis"
    }
}