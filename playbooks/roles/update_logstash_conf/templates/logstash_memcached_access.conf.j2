input {
  file{
    type=>"memcached-access"
    path=>"{{memcached_logs_path}}front-memcached.log"
    codec => json {
      charset => "UTF-8"
    }
  }
}
filter {
  if [type] == 'memcached-access' {
    json {
      source => "@fields"
    }
  }
}
output {
    redis {
      host => "{{redis_address}}"
      port => "{{redis_port}}"
      db => "0"
      data_type => "list"
      key => "memcached:redis"
    }
}